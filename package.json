{
  "name": "retax",
  "version": "0.0.0",
  "description": "Retax - React component",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "typings": "libTs/src/index.d.ts",
  "scripts": {
    "prepublish": "builder run build",
    "postinstall": "builder run typings:install",
    "build": "builder run build",
    "start": "builder run serve:playground",
    "server": "nodemon --exec 'builder run build:typescript && babel-node libTs/playground/serverEntry.jsx' --watch src --watch playground -e ts,tsx",
    "tdd": "builder run test:all:watch",
    "pretest": "builder run lint",
    "test": "builder run test:all:coverage",
    "lint": "builder run lint",
    "release": "builder run release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hourliert/retax"
  },
  "keywords": [
    "react",
    "component",
    "Retax",
    "seed"
  ],
  "author": "hourliert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hourliert/retax/issues"
  },
  "homepage": "https://github.com/hourliert/retax",
  "dependencies": {
    "builder-react-comp": "~0.1.6",
    "immutable": "^3.7.6",
    "inversify": "^2.0.0-alpha.6",
    "js-cookie": "^2.1.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "reflect-metadata": "^0.1.3"
  },
  "devDependencies": {
    "builder": "^2.9.1",
    "builder-react-comp-dev": "~0.1.6",
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "redux-logger": "^2.6.1"
  }
}
